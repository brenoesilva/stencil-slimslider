{
  "name": "stencil-slimslider",
  "version": "1.0.0",
  "description": "SlimSlider Carousel",
  "main": "dist/slimslider.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/slimslider.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@stencil/core": "^0.7.22",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shidhincr/stencil-slimslider.git"
  },
  "author": "Shidhin C R",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shidhincr/stencil-slimslider"
  },
  "homepage": "https://github.com/shidhincr/stencil-slimslider",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
